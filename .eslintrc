{
    "rules": {
      "react/prop-types": ["off"],
      "react/jsx-key": ["off"],
      "react/no-unknown-property":["off"],
      "react/display-name":["off"],
      "no-undef": ["off"],
      "react/react-in-jsx-scope": "error"
    },
    "extends": [
        "eslint:recommended", // Inherits rules from the recommended config
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
      "ecmaVersion": "latest", // Or set the specific ECMA version
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    }
  }